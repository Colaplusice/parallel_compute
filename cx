	return sd[t][m];
}
int Max(int i, int j) {
	if (i == 0) {
		max[i][j] = j*-8;
		return 0;
	}
	else if (j == 0) {
		max[i][j] = i*-8;
		return 0;
	}
			int ma1 = -9999;
			int ma2 = -9999;
	     	int ma3 = -9999;
			ma1 = max[i-1][j-1] + s(i, j);
					for (int j2 = 0; j2 <j; j2++) {
						if (max[i][j2]>ma2) {
							ma2 = max[i][j2];
						}
					}
					for (int t = 0; t <i; t++) {
						if (max[t][j]>ma3) {
							ma3 = max[t][j];
						}
					}
					ma2 = ma2 - 8;
					ma3 = ma3 - 8;
					max[i][j]= (ma1>ma2 ? (ma1 >ma3 ? ma1 : ma3) : (ma3 >ma2 ? ma3 : ma2));
	return 0;
}
void out() {
	
	printf("***********************************\n");
	for (int i = 0; i <=8; ++i)
	{
		for (int j = 0; j<=8; ++j)
		{
             printf("%d\t",max[i][j] );
		}
	  printf("\n");
	}
}
int main()
{
	int s = 2000;
	max = new int*[s];
	for (int i = 0; i <s; i++)
	{
		max[i] = new int[s];
		for (int j = 0; j < s; j++)
		{
			max[i][j] = 32767;
		}
	}
	
	int k=0;
	while(k<s){
		for (int i = 0; i <=k; ++i)
		{
			Max(i, k-i);
		}
        	k++;
	}
	out();
	return 0;
}


